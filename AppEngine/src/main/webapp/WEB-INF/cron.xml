<?xml version="1.0" encoding="UTF-8"?>
<cronentries
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/GoogleCloudPlatform/google-cloud-eclipse/master/plugins/com.google.cloud.tools.eclipse.appengine.validation/xsd/cron.xsd"
	>
	<cron>
		<url>/FeedCron?feed=TubeDepartureBoardsLineStatus</url>
		<description>Get Line Status</description>
		<schedule>every 5 minutes</schedule>
		<timezone>Europe/London</timezone>
	</cron>
	<cron>
		<url>/FeedCron?feed=TubeDepartureBoardsStationStatusIncidents</url>
		<description>Get Station Incidents</description>
		<schedule>every 5 minutes</schedule>
		<timezone>Europe/London</timezone>
	</cron>
</cronentries>
